{
  "name": "pebblejs",
  "author": "Meiguro",
  "version": "1.0.0",
  "repository": "pebble/pebblejs",
  "license": "MIT",
  "files": ["dist.zip"],
  "keywords": ["pebble-package"],
  "devDependencies": {
    "coffee-script": "^1.11.1"
  },
  "pebble": {
    "projectType": "package",
    "sdkVersion": "3",
    "capabilities": [
      "configurable"
    ],
    "targetPlatforms": [
      "aplite",
      "basalt",
      "chalk"
    ],
    "resources": {
      "media": [
        {
          "file": "images/menu_icon.png",
          "menuIcon": true,
          "name": "IMAGE_MENU_ICON",
          "type": "bitmap"
        },
        {
          "file": "images/logo_splash.png",
          "name": "IMAGE_LOGO_SPLASH",
          "type": "bitmap"
        },
        {
          "file": "images/tile_splash.png",
          "name": "IMAGE_TILE_SPLASH",
          "type": "bitmap"
        },
        {
          "file": "fonts/UbuntuMono-Regular.ttf",
          "name": "MONO_FONT_14",
          "type": "font"
        }
      ]
    }
  }
}
